  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EusRique/skeleton-node/issues"
  },
  "homepage": "https://github.com/EusRique/skeleton-node#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.20.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "git-commit-msg-linter": "^2.7.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.2",
    "typescript": "^3.9.9"
  },
